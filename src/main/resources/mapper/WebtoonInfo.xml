<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.woojaang.webtoon_recommendation.model.dao.WebtoonInfoDao">

	<select id="serchMoreThanOne" parameterType="String" resultType="WebtoonInfo"> 
		select TagName
		from tag
	</select>
	
	<select id="serchAll" parameterType="String" resultType="WebtoonInfo"> 
		select TagName
		from tag
	</select>
	
	<select id="test" resultType="WebtoonInfo"> 
		<!-- select webtoonNo, webtoonName, artist, star, completed, thumbnail, detailUrl, detail, tags -->
		select *
		from webtoon
		where webtoonName = #{tag};
		<!-- where webtoonNo like concat('%', #{tag}, '%');-->
	</select>
	
</mapper>